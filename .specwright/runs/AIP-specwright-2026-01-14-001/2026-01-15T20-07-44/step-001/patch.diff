++"""Work item CLI commands for Life.
++
++Thin wrappers around `life pm exec` for common work item operations.
++These normalize user-friendly inputs before delegating to pm.work_item.* ops.
++
++Copyright 2025 Ben Mensi
++Licensed under the Apache License, Version 2.0
++"""
++
++from __future__ import annotations
++
++import os
++import uuid
++from datetime import datetime, timezone
++from typing import Optional
++
++import typer
++from storacle.rpc import execute_plan
++from workman import compile as workman_compile
++from workman.errors import CompileError, ValidationError
++
++
++app = typer.Typer(
++    name="work",
++    help="Work item operations (wrappers around pm.work_item.*)",
++    no_args_is_help=True,
++)
++
++# Valid work item kinds (user can use lowercase, we normalize to uppercase)
++VALID_KINDS = {"TASK", "ISSUE", "CHANGE", "RISK", "DECISION", "MILESTONE", "OTHER"}
++
++
++def _get_actor() -> str:
++    """Get normalized actor from LIFE_ACTOR env var."""
++    actor = (os.environ.get("LIFE_ACTOR") or "").strip().lower()
++    if not actor:
++        raise typer.BadParameter("LIFE_ACTOR is required (set env var)")
++    return actor
++
++
++def _make_ctx(correlation_id: Optional[str]) -> dict:
++    """Build execution context."""
++    return {
++        "actor": _get_actor(),
++        "correlation_id": correlation_id or str(uuid.uuid4()),
++        "occurred_at": datetime.now(timezone.utc).isoformat(),
++        "producer": "life",
++    }
++
++
++def _normalize_kind(kind: str) -> str:
++    """Normalize kind to uppercase canonical form."""
++    normalized = kind.strip().upper()
++    if normalized not in VALID_KINDS:
++        valid = ", ".join(sorted(VALID_KINDS))
++        raise typer.BadParameter(f"Invalid kind '{kind}'. Valid: {valid}")
++    return normalized
++
++
++def _execute_op(op: str, payload: dict, correlation_id: Optional[str]) -> None:
++    """Compile and execute an operation, handling responses."""
++    try:
++        ctx = _make_ctx(correlation_id)
++    except typer.BadParameter as e:
++        typer.secho(str(e), err=True, fg=typer.colors.RED)
++        raise typer.Exit(code=1)
++
++    try:
++        plan = workman_compile(op, payload, ctx)
++    except ValidationError as e:
++        typer.secho(f"Invalid payload: {e}", err=True, fg=typer.colors.RED)
++        raise typer.Exit(code=1)
++    except CompileError as e:
++        typer.secho(f"Compile failed: {e}", err=True, fg=typer.colors.RED)
++        raise typer.Exit(code=1)
++
++    responses = execute_plan(plan)
++
++    if not responses:
++        typer.secho("No response", err=True, fg=typer.colors.RED)
++        raise typer.Exit(code=1)
++
++    resp = responses[0]
++    if "error" in resp:
++        code = resp["error"].get("code")
++        msg = resp["error"].get("message", "")
++        if code == -32001:
++            typer.secho(f"Assertion failed: {msg}", err=True, fg=typer.colors.RED)
++            raise typer.Exit(code=1)
++        if code == -32602:
++            typer.secho(f"Replay conflict: {msg}", err=True, fg=typer.colors.RED)
++            raise typer.Exit(code=1)
++        if code == -32003:
++            typer.secho(f"Storage error: {msg}", err=True, fg=typer.colors.RED)
++            raise typer.Exit(code=1)
++        typer.secho(f"Error ({code}): {msg}", err=True, fg=typer.colors.RED)
++        raise typer.Exit(code=1)
++
++    result = resp.get("result", {})
++    status = result.get("status")
++    event_id = result.get("event_id")
++
++    if status == "duplicate":
++        typer.echo(f"Idempotent replay (duplicate), event_id={event_id}")
++        raise typer.Exit(code=0)
++
++    if status == "created":
++        typer.echo(f"Created, event_id={event_id}")
++        raise typer.Exit(code=0)
++
++    typer.echo(f"OK: {result}")
++    raise typer.Exit(code=0)
++
++
